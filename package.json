  "scripts": {
    "clean": "yarn clean:client && yarn clean:server",
    "clean:client": "rimraf _client && mkdirp _client",
    "clean:server": "rimraf _server",
    "build": "yarn build:client && yarn build:server",
    "build:client": "yarn clean:client && cross-env NODE_ENV=production webpack --config webpack.client.js",
    "build:server": "yarn clean:server && cross-env NODE_ENV=production webpack --config webpack.server.js",
    "serve": "cross-env NODE_ENV=production PORT=8081 node ./server/server.production.js",
    "start": "yarn clean && cross-env NODE_ENV=development nodemon ./server/server.dev.js",
    "test": "jest",
    "lint": "eslint --fix --quiet server src server-renderer",
    "cm": "git-cz && git push",
    "pre:commit": "yarn lint && yarn test"
